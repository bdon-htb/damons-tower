{
  "inputCommands": {
    "doubleTap-right": {
      "pattern": ["keyDown-right", "keyUp-right", "keyDown-right"]
    },
    "doubleTap-left": {
      "pattern": ["keyDown-left", "keyUp-left", "keyDown-left"]
    },
    "doubleTap-up": {
      "pattern": ["keyDown-up", "keyUp-up", "keyDown-up"]
    },
    "doubleTap-down": {
      "pattern": ["keyDown-down", "keyUp-down", "keyDown-down"]
    },
    "singleTap-space": {
      "pattern": ["keyDown-space", "keyUp-space"],
      "type": "tap",
      "timeLimit": "none"
    }
  }
}
