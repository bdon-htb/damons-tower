<?xml version="1.0" encoding="UTF-8"?>
<file>
  <header>
      <type>menu</type>
      <name>debugMenu</name>
      <layout>floatLayout</layout>
  </header>
  <menu>
      <label text="Development build" x="0" y="0"></label>
      <list x="480" y="100" spaceBetween="10" justifyContents="center">
        <img type="gui" src="titleLogo" justifyContents="center"></img>
        <arrowSelect options="[960x540, 1280x720, Fullscreen]"></arrowSelect>
        <button text="Start Game" callback="startGame"></button>
        <button text="Toggle Fullscreen" callback="resizeTest"></button>
        <button text="1280x720" callback="resizeTest2"></button>
        <button text="960x540" callback="resizeTest3"></button>
      </list>
  </menu>
</file>
